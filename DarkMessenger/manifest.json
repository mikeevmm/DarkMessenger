{
    "manifest_version" : 2,
    "name" : "Dark Messenger",
    "version" : "0.1",
    "content_scripts": [
    {
        "matches": [
        "https://www.messenger.com/*",
        "http://www.messenger.com/*"
        ],
        "js": ["jquery-3.2.1.min.js","content.js"]
    }
    ],
    "background":{
        "scripts" : ["background.js"]
    },
    "browser_action": {},
    "permissions":[
        "storage",
        "tabs"
    ]
}